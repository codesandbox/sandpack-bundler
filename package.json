{
  "name": "sandpack-bundler",
  "version": "2.0.0",
  "description": "The bundler powering sandpack",
  "license": "Apache-2.0",
  "author": "CodeSandbox",
  "keywords": [],
  "repository": {
    "type": "git",
    "url": "https://github.com/codesandbox/sandpack-bundler"
  },
  "scripts": {
    "prepare": "husky install",
    "dev": "parcel ./src/index.html",
    "start": "node ./server.js",
    "build": "parcel build ./src/index.html",
    "test": "jest",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write \"./src/**/*.{ts,tsx,js,jsx,json}\""
  },
  "importSort": {
    ".js, .jsx, .ts, .tsx": {
      "style": "module",
      "parser": "typescript"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx,json}": [
      "pretty-quick --write"
    ]
  },
  "browserslist": "> 2.5%, not ie 11, not dead, not op_mini all",
  "dependencies": {
    "@babel/standalone": "7.17.5",
    "@swc/helpers": "^0.3.8",
    "@types/babel__standalone": "7.1.3",
    "gensync": "^1.0.0-beta.2",
    "micromatch": "^4.0.4",
    "p-queue": "^6.6.2",
    "postcss": "^8.4.6",
    "postcss-import": "^14.0.2",
    "react": "^17.0.2",
    "react-devtools-inline": "^4.22.0",
    "react-devtools-inline_legacy": "npm:react-devtools-inline@4.4.0",
    "react-refresh": "^0.11.0",
    "strip-json-comments": "^4.0.0",
    "url-join": "^4.0.1"
  },
  "devDependencies": {
    "@swc-node/jest": "^1.4.3",
    "@types/jest": "^27.4.0",
    "@types/micromatch": "^4.0.2",
    "@types/url-join": "^4.0.1",
    "@types/uuid": "^8.3.4",
    "fastify": "^3.27.0",
    "fastify-static": "^4.5.0",
    "husky": "^7.0.4",
    "import-sort-style-module": "^6.0.0",
    "jest": "^27.5.1",
    "jest-extended": "^2.0.0",
    "parcel": "^2.0.1",
    "prettier": "^2.5.1",
    "prettier-plugin-import-sort": "^0.0.7",
    "pretty-quick": "^3.1.3",
    "typescript": "4.4.4"
  },
  "resolutions": {
    "@babel/preset-env": "7.13.8"
  }
}
